{
    "version": 2,
    "builds": [
      {
        "src": "./Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./Swagger.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Server.js"
      },
      {
        "src": "/(.*)",
        "dest": "Swagger.js"
      }
    ]
  }